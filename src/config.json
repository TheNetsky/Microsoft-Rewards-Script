{
    "baseURL": "https://rewards.bing.com",
    "sessionPath": "sessions",
    "headless": false,
    "parallel": false,
    "runOnZeroPoints": false,
    "clusters": 1,
    "saveFingerprint": {
        "mobile": false,
        "desktop": false
    },
    
    "searchOnBingLocalQueries": false,
    "globalTimeout": "30s",
    "searchSettings": {
        "useGeoLocaleQueries": false,
        "scrollRandomResults": true,
        "clickRandomResults": true,
        "searchDelay": {
            "min": "3min",
            "max": "5min"
        },
        "retryMobileSearchAmount": 2
    },
    "humanization": {
        "actionDelay": { "min": 150, "max": 450 },
        "gestureMoveProb": 0.4,
        "gestureScrollProb": 0.2,
        "allowedWindows": []
    },
    "retryPolicy": {
        "maxAttempts": 3,
        "baseDelay": 1000,
        "maxDelay": "30s",
        "multiplier": 2,
        "jitter": 0.2
    },
    "jobState": {
        "enabled": true,
        "dir": ""
    },
    "logExcludeFunc": [
        "SEARCH-CLOSE-TABS",
        "LOGIN-NO-PROMPT",
        "FLOW"
    ],
    "webhookLogExcludeFunc": [
        "SEARCH-CLOSE-TABS",
        "LOGIN-NO-PROMPT",
        "FLOW"
    ],
    "proxy": {
        "proxyGoogleTrends": true,
        "proxyBingTerms": true
    },
    "webhook": {
        "enabled": false,
        "url": ""
    },
    "conclusionWebhook": {
        "enabled": false,
        "url": ""
    },
    "ntfy": {
        "enabled": false,
        "url": "",  
        "topic": "rewards", 
        "authToken": ""  
    },
    "diagnostics": {
        "enabled": false,
        "saveScreenshot": true,
        "saveHtml": true,
        "maxPerRun": 2,
        "retentionDays": 7
    },
    "update": {
        "git": true,
        "docker": false,
        "scriptPath": "setup/update/update.mjs"
    },
    "schedule": {
        "enabled": false,
        "time": "09:00",
        "timeZone": "America/New_York",
        "runImmediatelyOnStart": true
    },
    "passesPerRun": 1,
    "workers": {"doDailySet": true, "doMorePromotions": true, "doPunchCards": true, "doDesktopSearch": true, "doMobileSearch": true, "doDailyCheckIn": true, "doReadToEarn": true, "bundleDailySetWithSearch": false}
}